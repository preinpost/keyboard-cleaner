{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "keyboard-cleaner",
  "version": "0.1.0",
  "identifier": "com.keyboard-cleaner.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "keyboard-cleaner",
        "width": 500,
        "height": 300
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.png",
      "icons/icon.icns"
    ]
  }
}
